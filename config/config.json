{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "my-request-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/myRequest/MyRequestWebPart.js",
          "manifest": "./src/webparts/myRequest/MyRequestWebPart.manifest.json"
        }
      ]
    },
    "my-approval-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/myApproval/MyApprovalWebPart.js",
          "manifest": "./src/webparts/myApproval/MyApprovalWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "globalName": "bootstrap"
    }
  },
  "localizedResources": {
   "MyRequestWebPartStrings": "lib/webparts/myRequest/loc/{locale}.js",
    "MyApprovalWebPartStrings": "lib/webparts/myApproval/loc/{locale}.js"
  }
}
